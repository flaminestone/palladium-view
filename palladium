server {
  listen 80;
   root //home/flamine/WebstormProjects/palladium-view/dist/;
   index index.html index.htm;
   server_name  palladium;
  location / {
    try_files $uri $uri/ =404;
  }

  location /api {
    proxy_pass http://0.0.0.0:9292;
  }

  location /login {
    proxy_pass http://0.0.0.0:9292;
  }

  location /registration {
    proxy_pass http://0.0.0.0:9292;
   }
}
